<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="pos_functions_form_view" model="ir.ui.view">
            <field name="name">pos_functions.form</field>
            <field name="model">pos.functions</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="bank_id"/>
                            <field name="name"/>
                            <field name="disable"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="pos_functions_tree_view" model="ir.ui.view">
            <field name="name">pos_functions.tree</field>
            <field name="model">pos.functions</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="bank_id"/>
                    <field name="name"/>
                    <field name="disable"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>